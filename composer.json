{
    "name": "scripturadesign/quickstart",
    "description": "Quickstart a project. Very opinionated to the way I prefer currently.",
    "keywords": ["installer", "boilerplate", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Dilling-Hansen",
            "email": "martindilling@gmail.com",
            "homepage": "http://martindilling.com"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "guzzlehttp/guzzle": "^6.0",
        "league/flysystem": "^1.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/filesystem": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/quickstart"
    ],
    "autoload": {
        "psr-4": {
            "Scriptura\\QuickStart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scriptura\\QuickStart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
